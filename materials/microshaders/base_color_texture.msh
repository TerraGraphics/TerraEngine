name: "BASE_COLOR_TEXTURE"
group: "BASE_COLOR_VALUE"
pixel {
    parameters: {
        textures2D: ["texBase"]
        inputs: {
            uv: [float2, TEX_COORD]
        }
    }
    source: <<SHADER
float4 GetBaseColor(in PSInput psIn) {
    return texBase.Sample(texBaseSampler, psIn.uv);
}
SHADER
}
